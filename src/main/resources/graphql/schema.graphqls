scalar BigDecimal  #currently not used
scalar LocalDateTime

type Student{
    id:ID!
    firstName:String
    lastName:String
    email:String
    age:Int
    dateCreated:LocalDateTime
    dateUpdated:LocalDateTime
}

type Query{
    students:[Student]
}


